<header>
    <div class="start">
        <button class="menu"  onclick="onClickSideMenu()"><img src="image/menu.svg"></button>
        <a class="logo" href="/"><img class="logo-icon" src="image/logo.png"></a>
    </div>
    <div class="center">
        <form class="search-container">
            <input class="search" type="text" placeholder="검색">
            <button class="search-button" >
                <img src="image/search.svg">
            </button>
        </form>
    </div>
    <div class="end">
        <% if (user.isLogined) { %>
            <a href="/member/logout">로그아웃</a>
        <% } else { %>
            <a href="/member/login">로그인</a>
            <div>&nbsp/&nbsp</div>
            <a href="/member/signup">회원가입</a>
        <% } %>
    </div>
</header>